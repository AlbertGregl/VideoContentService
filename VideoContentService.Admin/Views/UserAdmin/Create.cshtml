@model VideoContentService.Admin.Models.UserRequest

@{
    ViewBag.Title = "Create User";
}

<h2>Create User</h2>

<form asp-controller="UserAdmin" asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="FirstName">First Name:</label>
        <input asp-for="FirstName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="LastName">Last Name:</label>
        <input asp-for="LastName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Email">Email:</label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Phone">Phone:</label>
        <input asp-for="Phone" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="CountryOfResidenceId">Country ID:</label>
        <input asp-for="CountryOfResidenceId" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>

